{
  "expo": {
    "name": "@privy-io/expo-starter",
    "slug": "privy-expo-starter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "scheme": "privy-expo",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "io.example.expo",
      "supportsTablet": true
    },
    "android": {
      "package": "io.example.expo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "privyAppId": "<enter your app id>",
      "privyClientId": "<enter your client id>"
    },
    "plugins": ["expo-secure-store"]
  }
}
