{
  "expo": {
    "name": "@privy-io/expo-starter",
    "slug": "privy-expo-starter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "scheme": "privy-expo",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.jainil.expo",
      "supportsTablet": true
    },
    "android": {
      "package": "com.jainil.expo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "privyAppId": "clwuz57hc0a7xpjf6yz2a566i"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
